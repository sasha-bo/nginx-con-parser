{
    "name": "sashabo/nginx-conf-parser",
    "version": "1.2.1",
    "description": "Parsing nginx.conf files and following includes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SashaBo\\NginxConfParser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sasha Bo"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.25",
        "phpunit/phpunit": "^10"
    }
}
